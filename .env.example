# Copy this file to .env and edit the settings

# Port to listen on (example: 3000)
PORT=

# MongoDB database URL (example: mongodb://localhost/dbname)
DATABASE_URL=

# Session secret string (must be unique to your server)
SESSION_SECRET=

# OpenAI API Key
OPENAI_API_KEY= # {insert your OpenAI API key here} # OpenAI API key is required for processing

# LangChain API URL
LANGCHAIN_URL= # {insert your LangChain API URL here} # LangChain API URL is required for processing

# Github Actions URL
GITHUB_ACTIONS_URL= # {insert your Github Actions URL here} # GitHub Actions URL is required for CI/CD integration

# Notification Service API Key
NOTIFICATION_SERVICE_API_KEY= # {insert your Notification Service API key here} # Notification Service API key for sending notifications

# Notification Service Webhook URL
NOTIFICATION_SERVICE_WEBHOOK_URL= # {insert your Notification Service Webhook URL here} # Notification Service Webhook URL for sending notifications